{
	"name": "coffee-shop-by-section",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:ci": "vitest run --coverage",
    "type-check": "svelte-check --tsconfig ./jsconfig.json",
    "start:preview": "vite preview --port 4173 --strictPort",
    "test:e2e": "npm run build && playwright test",
    "test:e2e:ci": "npm run build && playwright test",
    "load:test": "start-server-and-test start:preview http://localhost:4173 autocannon -d 10 -c 20 http://localhost:4173/",
    "load:test:ci": "start-server-and-test start:preview http://localhost:4173 autocannon -d 10 -c 20 http://localhost:4173/"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
    "@playwright/test": "1.54.2",
    "autocannon": "7.15.0",
    "start-server-and-test": "2.0.13",
		"@chromatic-com/storybook": "^3",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@storybook/addon-essentials": "^8.6.12",
		"@storybook/addon-svelte-csf": "^5.0.0-next.0",
		"@storybook/blocks": "^8.6.12",
		"@storybook/experimental-addon-test": "^8.6.12",
		"@storybook/svelte": "^8.6.12",
		"@storybook/sveltekit": "^8.6.12",
		"@storybook/test": "^8.6.12",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/postcss": "^4.1.5",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.1.5",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^22",
		"@vitest/browser": "3.1.2",
		"@vitest/coverage-v8": "3.1.2",
		"autoprefixer": "^10.4.21",
		"daisyui": "^5.0.35",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"mdsvex": "^0.12.3",
    "playwright": "1.54.2",
		"postcss": "^8.5.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"storybook": "^8.6.12",
		"svelte": "^5.28.2",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.5",
		"typescript": "^5.0.0",
		"vite": "^6.3.4",
		"vitest": "^3.0.0"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@stripe/stripe-js": "^7.8.0",
		"@supabase/supabase-js": "^2.49.4",
		"chart.js": "^4.4.9",
    "stripe": "^18.4.0",
    "svelte": "^5.0.0",
    "vite": "^6.3.4",
    "web-vitals": "4.2.4"
	}
}
